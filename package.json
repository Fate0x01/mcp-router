{
  "name": "mcp-router",
  "version": "0.5.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mcp-router/mcp-router"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "dev": "pnpm run dev:electron",
    "dev:web": "turbo run dev --filter=@mcp-router/web",
    "dev:electron": "turbo run dev --filter=@mcp-router/shared --filter=@mcp-router/platform-api --filter=@mcp-router/ui --filter=@mcp-router/electron",
    "make": "turbo run make",
    "publish": "turbo run publish",
    "build": "turbo run build",
    "start": "pnpm --filter @mcp-router/electron run start",
    "lint": "turbo run lint",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs --fix",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "lint:all": "turbo run lint",
    "format:all": "turbo run format"
  },
  "keywords": [],
  "author": {
    "name": "fjm2u",
    "email": "fm.job@icloud.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  },
  "pnpm": {
    "overrides": {
      "webpack-dev-server": "^5.2.1"
    }
  }
}
