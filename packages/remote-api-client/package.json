{
  "name": "@mcp-router/remote-api-client",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.mjs",
      "require": "./dist/schema/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@trpc/client": "^10.45.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@mcp-router/shared": "workspace:*",
    "@types/node": "^20.14.2",
    "eslint": "^9.6.0",
    "tsup": "^8.0.2",
    "typescript": "^5.8.3"
  }
}